# Default configuration for RustNEMU
# This file is auto-generated from Kconfig

CONFIG_ISA="riscv32"
CONFIG_ISA_riscv=y

# Build Options
CONFIG_CC_OPT="-O3"

# Testing and Debugging
CONFIG_TRACE=y
CONFIG_TRACE_START=0
CONFIG_TRACE_END=10000
CONFIG_ITRACE=y
CONFIG_ITRACE_COND="true"
CONFIG_ITRACE_RINGBUF=32
CONFIG_MTRACE=y
CONFIG_MTRACE_COND="true"
CONFIG_MTRACE_RINGBUF=32
CONFIG_FTRACE=y
CONFIG_FTRACE_COND="true"
CONFIG_FTRACE_BUF=1024
CONFIG_DTRACE=y
CONFIG_DTRACE_COND="true"
CONFIG_DTRACE_RINGBUF=32
CONFIG_WATCHPOINT=y

# Memory Configuration
CONFIG_MBASE=0x80000000
CONFIG_MSIZE=0x8000000
CONFIG_PC_RESET_OFFSET=0
CONFIG_MEM_RANDOM=y

# Devices
CONFIG_DEVICE=y
CONFIG_HAS_SERIAL=y
CONFIG_SERIAL_MMIO=0xa00003f8
CONFIG_HAS_TIMER=y
CONFIG_RTC_MMIO=0xa0000048
CONFIG_HAS_KEYBOARD=y
CONFIG_I8042_DATA_MMIO=0xa0000060
CONFIG_HAS_VGA=y
CONFIG_FB_ADDR=0xa1000000
CONFIG_VGA_CTL_MMIO=0xa0000100
CONFIG_VGA_SHOW_SCREEN=y
CONFIG_HAS_AUDIO=y
CONFIG_SB_ADDR=0xa1200000
CONFIG_SB_SIZE=0x10000
CONFIG_AUDIO_CTL_MMIO=0xa0000200
CONFIG_HAS_DISK=y
CONFIG_DISK_CTL_MMIO=0xa0000300
CONFIG_DISK_IMG_PATH=""
CONFIG_HAS_CLINT=y
CONFIG_HAS_PLIC=y
